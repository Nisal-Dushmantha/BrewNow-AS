<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_constraintCircleRadius="@android:dimen/notification_large_icon_height"
    tools:context=".MenuActivity">

    <FrameLayout
        android:id="@+id/bottom"
        android:layout_width="415dp"
        android:layout_height="111dp"
        android:layout_marginTop="780dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/home"
                android:layout_width="50dp"
                android:layout_height="80dp"
                android:layout_weight="1"
                android:paddingTop="30dp"
                app:srcCompat="@drawable/home"
                android:clickable="true"
                android:focusable="true"/>

            <ImageView
                android:id="@+id/offers"
                android:layout_width="50dp"
                android:layout_height="80dp"
                android:layout_weight="1"
                android:paddingTop="30dp"
                app:srcCompat="@drawable/offers"
                android:clickable="true"
                android:focusable="true"/>

            <ImageView
                android:id="@+id/favourite"
                android:layout_width="50dp"
                android:layout_height="80dp"
                android:layout_weight="1"
                android:paddingTop="30dp"
                app:srcCompat="@drawable/heart"
                android:clickable="true"
                android:focusable="true"/>

            <ImageView
                android:id="@+id/cart"
                android:layout_width="50dp"
                android:layout_height="80dp"
                android:layout_weight="1"
                android:paddingTop="30dp"
                app:srcCompat="@drawable/cart"
                android:clickable="true"
                android:focusable="true"/>

        </LinearLayout>
    </FrameLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/content"
        android:layout_width="415dp"
        android:layout_height="651dp"
        android:layout_marginTop="126dp"
        android:layout_marginBottom="95dp"
        android:background="@color/primary_color"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.75"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0">

        <ImageView
            android:id="@+id/imageView2"
            android:layout_width="412dp"
            android:layout_height="352dp"
            android:layout_marginEnd="1dp"
            android:layout_marginBottom="336dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toStartOf="parent"
            app:srcCompat="@drawable/cappaccino__4_" />

        <ImageView
            android:id="@+id/imageView3"
            android:layout_width="411dp"
            android:layout_height="386dp"
            android:layout_marginStart="2dp"
            android:layout_marginTop="352dp"
            android:layout_marginEnd="2dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/imageView2"
            app:srcCompat="@drawable/cappaccino__5_" />

        <ImageView
            android:id="@+id/imageView"
            android:layout_width="414dp"
            android:layout_height="355dp"
            android:layout_marginTop="208dp"
            android:layout_marginEnd="1dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/cappaccino__3_" />

        <Button
            android:id="@+id/espresso"
            android:layout_width="106dp"
            android:layout_height="46dp"
            android:layout_marginStart="271dp"
            android:layout_marginTop="67dp"
            android:layout_marginEnd="38dp"
            android:layout_marginBottom="538dp"
            android:backgroundTint="@color/secondary_color"
            android:text="order"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <Button
            android:id="@+id/americano"
            android:layout_width="106dp"
            android:layout_height="46dp"
            android:layout_marginStart="271dp"
            android:layout_marginTop="182dp"
            android:layout_marginEnd="38dp"
            android:layout_marginBottom="423dp"
            android:backgroundTint="@color/secondary_color"
            android:text="order"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <Button
            android:id="@+id/cappaccino"
            android:layout_width="106dp"
            android:layout_height="46dp"
            android:layout_marginStart="271dp"
            android:layout_marginTop="315dp"
            android:layout_marginEnd="38dp"
            android:layout_marginBottom="290dp"
            android:backgroundTint="@color/secondary_color"
            android:text="order"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <Button
            android:id="@+id/blackCoffee"
            android:layout_width="106dp"
            android:layout_height="46dp"
            android:layout_marginStart="271dp"
            android:layout_marginTop="434dp"
            android:layout_marginEnd="38dp"
            android:layout_marginBottom="171dp"
            android:backgroundTint="@color/secondary_color"
            android:text="order"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <Button
            android:id="@+id/mocha"
            android:layout_width="106dp"
            android:layout_height="46dp"
            android:layout_marginStart="271dp"
            android:layout_marginTop="554dp"
            android:layout_marginEnd="38dp"
            android:layout_marginBottom="51dp"
            android:backgroundTint="@color/secondary_color"
            android:text="order"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <LinearLayout
        android:layout_width="417dp"
        android:layout_height="122dp"
        android:layout_marginTop="1dp"
        android:layout_marginBottom="768dp"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:id="@+id/imageView11"
            android:layout_width="133dp"
            android:layout_height="match_parent"
            android:paddingLeft="25dp"
            android:paddingTop="25dp"
            android:paddingEnd="25dp"
            android:paddingRight="25dp"
            android:paddingBottom="25dp"
            app:srcCompat="@drawable/logo" />

        <TextView
            android:id="@+id/textView2"
            android:layout_width="171dp"
            android:layout_height="51dp"
            android:layout_marginTop="30dp"
            android:layout_weight="1"
            android:text="BrewNow"
            android:textSize="30dp"
            android:textStyle="bold" />

        <ImageView
            android:id="@+id/imageView13"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:layout_marginTop="35dp"
            android:layout_weight="1"
            app:srcCompat="@drawable/profile" />

    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>