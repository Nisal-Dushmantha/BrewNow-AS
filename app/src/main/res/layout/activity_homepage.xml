<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_constraintCircleRadius="@android:dimen/notification_large_icon_height"
    tools:context=".HomepageActivity">

    <FrameLayout
        android:id="@+id/bottom"
        android:layout_width="415dp"
        android:layout_height="111dp"
        android:layout_marginTop="780dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/home"
                android:layout_width="50dp"
                android:layout_height="80dp"
                android:layout_weight="1"
                android:clickable="true"
                android:focusable="true"
                android:paddingTop="30dp"
                app:srcCompat="@drawable/home" />

            <ImageView
                android:id="@+id/offers"
                android:layout_width="50dp"
                android:layout_height="80dp"
                android:layout_weight="1"
                android:clickable="true"
                android:focusable="true"
                android:paddingTop="30dp"
                app:srcCompat="@drawable/offers" />

            <ImageView
                android:id="@+id/favourite"
                android:layout_width="50dp"
                android:layout_height="80dp"
                android:layout_weight="1"
                android:clickable="true"
                android:focusable="true"
                android:paddingTop="30dp"
                app:srcCompat="@drawable/heart" />

            <ImageView
                android:id="@+id/cart"
                android:layout_width="50dp"
                android:layout_height="80dp"
                android:layout_weight="1"
                android:clickable="true"
                android:focusable="true"
                android:paddingTop="30dp"
                app:srcCompat="@drawable/cart" />

        </LinearLayout>
    </FrameLayout>

    <FrameLayout
        android:id="@+id/content"
        android:layout_width="415dp"
        android:layout_height="651dp"
        android:layout_marginTop="130dp"
        android:layout_marginBottom="110dp"
        android:background="@color/primary_color"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.25"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/frameLayout"
            android:layout_width="match_parent"
            android:layout_height="285dp">

            <TextView
                android:id="@+id/textView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="184dp"
                android:layout_marginBottom="226dp"
                android:text="Featured Drinks"
                android:textSize="30dp"
                android:textStyle="bold"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                tools:layout_marginLeft="20dp" />

            <ImageView
                android:id="@+id/imageView2"
                android:layout_width="168dp"
                android:layout_height="269dp"
                android:layout_marginStart="16dp"
                android:layout_marginTop="69dp"
                android:layout_marginEnd="235dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:srcCompat="@drawable/white"
                tools:layout_marginTop="80dp" />

            <ImageView
                android:id="@+id/imageView5"
                android:layout_width="168dp"
                android:layout_height="269dp"
                android:layout_marginStart="234dp"
                android:layout_marginTop="49dp"
                android:layout_marginEnd="13dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:srcCompat="@drawable/white"
                tools:layout_marginTop="80dp" />

            <ImageView
                android:id="@+id/imageView"
                android:layout_width="165dp"
                android:layout_height="218dp"
                android:layout_marginStart="16dp"
                android:layout_marginEnd="234dp"
                android:layout_marginBottom="48dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:srcCompat="@drawable/cappuccino_at_sightglass_coffee" />

            <TextView
                android:id="@+id/textView3"
                android:layout_width="137dp"
                android:layout_height="31dp"
                android:layout_marginStart="26dp"
                android:layout_marginTop="171dp"
                android:layout_marginEnd="252dp"
                android:layout_marginBottom="64dp"
                android:text="Cappacino"
                android:textSize="25dp"
                android:textStyle="bold"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/textView5"
                android:layout_width="137dp"
                android:layout_height="31dp"
                android:layout_marginStart="245dp"
                android:layout_marginTop="180dp"
                android:layout_marginEnd="33dp"
                android:layout_marginBottom="74dp"
                android:text="Espresso"
                android:textSize="25dp"
                android:textStyle="bold"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/textView4"
                android:layout_width="67dp"
                android:layout_height="27dp"
                android:layout_marginStart="26dp"
                android:layout_marginTop="209dp"
                android:layout_marginEnd="322dp"
                android:layout_marginBottom="30dp"
                android:text="Rs.350"
                android:textColor="@color/accent_color"
                android:textSize="20dp"
                android:textStyle="bold"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.105" />

            <TextView
                android:id="@+id/textView6"
                android:layout_width="67dp"
                android:layout_height="27dp"
                android:layout_marginStart="245dp"
                android:layout_marginTop="211dp"
                android:layout_marginEnd="103dp"
                android:layout_marginBottom="47dp"
                android:text="Rs.350"
                android:textColor="@color/accent_color"
                android:textSize="20dp"
                android:textStyle="bold"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <ImageView
                android:id="@+id/imageView3"
                android:layout_width="165dp"
                android:layout_height="228dp"
                android:layout_marginStart="234dp"
                android:layout_marginTop="1dp"
                android:layout_marginEnd="16dp"
                android:layout_marginBottom="56dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:srcCompat="@drawable/espresso_cup_with_coffee_beans" />

            <RadioButton
                android:id="@+id/radioButton"
                android:layout_width="26dp"
                android:layout_height="25dp"
                android:layout_marginStart="136dp"
                android:layout_marginTop="213dp"
                android:layout_marginEnd="253dp"
                android:layout_marginBottom="47dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <RadioButton
                android:id="@+id/radioButton2"
                android:layout_width="26dp"
                android:layout_height="25dp"
                android:layout_marginStart="355dp"
                android:layout_marginTop="213dp"
                android:layout_marginEnd="34dp"
                android:layout_marginBottom="47dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

        </androidx.constraintlayout.widget.ConstraintLayout>
    </FrameLayout>

    <LinearLayout
        android:layout_width="417dp"
        android:layout_height="122dp"
        android:layout_marginTop="1dp"
        android:layout_marginBottom="768dp"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:id="@+id/imageView11"
            android:layout_width="133dp"
            android:layout_height="match_parent"
            android:paddingLeft="25dp"
            android:paddingTop="25dp"
            android:paddingEnd="25dp"
            android:paddingRight="25dp"
            android:paddingBottom="25dp"
            app:srcCompat="@drawable/logo" />

        <TextView
            android:id="@+id/textView2"
            android:layout_width="171dp"
            android:layout_height="51dp"
            android:layout_marginTop="30dp"
            android:layout_weight="1"
            android:text="BrewNow"
            android:textSize="30dp"
            android:textStyle="bold" />

        <ImageView
            android:id="@+id/imageView13"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:layout_marginTop="35dp"
            android:layout_weight="1"
            app:srcCompat="@drawable/profile" />

    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>